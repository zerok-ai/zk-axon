            "get": {
                "description": "Get Incidents grouped by scenario title, destination and scenario type with pagination for given source",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "trace persistence"
                ],
                "summary": "Get Incidents",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Scenario Type",
                        "name": "scenarioType",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Source",
                        "name": "source",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Limit",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Offset",
                        "name": "offset",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/scenariodataresponse.IncidentResponse"
                        }
                    }
                }
            }
        },
